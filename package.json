{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "build": "medusa build && node scripts/apply-patches.js",
    "predeploy": "medusa db:migrate",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
 
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@medusajs/admin-sdk": "2.12.1",
    "@medusajs/cli": "2.12.1",
    "@medusajs/draft-order": "^2.12.1",
    "@medusajs/framework": "2.12.1",
    "@medusajs/medusa": "2.12.1",
    "@mikro-orm/postgresql": "^6.4.16",
    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "lucide-react": "^0.555.0",
    "patch-package": "^8.0.1"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.1",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
